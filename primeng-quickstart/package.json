{
  "name": "primeng-quickstart",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "format": "prettier --write .",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "e2e:headless": "npx playwright install --with-deps && cross-env HEADLESS=1 ng e2e --configuration production"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20",
    "@angular/compiler": "^20",
    "@angular/core": "^20",
    "@angular/forms": "^20",
    "@angular/platform-browser": "^20",
    "@angular/platform-browser-dynamic": "^20",
    "@angular/router": "^20",
    "@microsoft/signalr": "^9.0.6",
    "@primeuix/themes": "^1.2.0",
    "chart.js": "^4.4.8",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.0.0-rc.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20",
    "@angular/cli": "^20",
    "@angular/compiler-cli": "^20",
    "@playwright/test": "1.56.0",
    "@types/jasmine": "~5.1.0",
    "@types/underscore": "^1.13.0",
    "angular-eslint": "20.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "playwright-ng-schematics": "^2.1.1",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "8.40.0"
  }
}
